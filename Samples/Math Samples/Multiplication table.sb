For I = 1 To 12
   For J = 1 To 12
      TW.WriteFormatted(
         "[1] x [2] = [3][4]",
         {
            Text.PadRight(I, 2),
            Text.PadLeft(J, 2),
            Text.PadLeft(I * J, 3),
            Text.NewLine
         }
      )
   Next
   TW.WriteLine("-------------------------")
Next
