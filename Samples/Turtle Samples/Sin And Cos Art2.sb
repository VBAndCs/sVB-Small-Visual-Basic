Math.UseRadianAngles = False
Turtle.Speed = 50
GW.BrushColor = Colors.Blue

Rx = 450
Ry = 200
Cx = 500
Cy = 300
Turtle.CreateFigure()

For A = 0 To 360
   R1 = Rx * Math.Cos(20 * A) + 10 * Math.Sin(360 * Math.Cos(A))
   X = R1 * Math.Cos(A)
   R2 = Ry * Math.Cos(320 * A) + 10 * Math.Sin(360 * Math.Sin(A))
   Y = R2 * Math.Sin(A)
   If A = 0 Then
      Turtle.X = Cx + X
      Turtle.Y = Cy + Y
   EndIf
   Turtle.MoveTo(Cx + X, Cy + Y)
Next

Turtle.Hide()
Turtle.FillFigure()
