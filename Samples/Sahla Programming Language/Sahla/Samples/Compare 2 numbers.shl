# مثال مقارنة عددين:
اكتب "أدخل العدد الأول: "
ع1 = اقرأ عددا
اكتب "أدخل العدد الثاني: "
ع2 = اقرأ عددا

لو ع1 > ع2
    اكتب السطر "العدد الأول (" + ع1 + ") أكبر من العدد الثاني (" + ع2 + ")"
أو لو ع1 < ع2
    اكتب السطر "العدد الأول (" + ع1 + ") أصغر من العدد الثاني (" + ع2 + ")"
وإلا
    اكتب السطر "العددان متساويان وقيمتهما = " + ع1
