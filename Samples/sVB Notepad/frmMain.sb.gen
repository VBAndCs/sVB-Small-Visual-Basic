FrmMain = Forms.LoadForm("FrmMain", _path)
Form.SetArgsArr(frmmain, Stack.PopValue("_frmmain_argsArr"))
Form.Show(FrmMain)
'#Events{
'    TxtEditor: OnTextChanged
