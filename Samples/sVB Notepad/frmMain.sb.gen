'}

' TxtEditor Events:
Control.HandleEvents(TxtEditor)
TextBox.OnTextChanged = TxtEditor_OnTextChanged

' FrmMain Events:
Control.HandleEvents(FrmMain)
Form.OnClosing = FrmMain_OnClosing
Control.OnPreviewKeyDown = FrmMain_OnPreviewKeyDown


