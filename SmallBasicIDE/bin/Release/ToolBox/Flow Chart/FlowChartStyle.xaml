    <Style x:Key="Terminator"
           TargetType="Path"
           BasedOn="{StaticResource FlowChartItemStyle}">
        <Setter Property="Data"
                Value="M 20,40 A 20,20 0 0 1 20,0 H 40 A 20,20 0 0 1 40,40 Z" />
    </Style>

    <Style x:Key="Terminator_DragThumb"
